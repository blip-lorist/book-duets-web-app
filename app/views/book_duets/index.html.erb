<p><%= link_to "Suggested Pairing", suggested_pairing_path, class: "btn btn-success" %></p>
<!-- Button here that loads a page with a suggested pairing -->

<!-- Fields for creating a custom duet -->
<%= form_tag custom_duet_redirect_path :class => "form-horizontal" do %>
<div class="form-group">
  <%= label_tag :musician %>
  <div class="col-sm-10">
    <%= text_field_tag :musician %>
  </div>
</div>
<div class="form-group">
  <%= label_tag :author%>
  <div class="col-sm-10">
    <%= text_field_tag :author %>
  </div>
</div>
<div class="form-group">
  <div class="col-sm-offset-2 col-sm-10">
    <%= submit_tag 'SANG IT', :class => "btn btn-success"%>
  </div>
</div>
<% end %>
