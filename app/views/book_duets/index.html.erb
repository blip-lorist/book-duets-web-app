<div class="panel panel-primary">
  <div class="panel-heading"><h3>Book Duets Feed</h3></div>
  <div class="panel-body">
    <% if @book_duets.count == 0 %>
      <h4>No one has saved a single Book Duet yet! Why don't you do something about that?</h4>
    <% else %>
      <ul class="list-group">
        <% @book_duets.each do |book_duet| %>
          <li class="list-group-item"><h4><strong><%= book_duet.user.username %></strong> saved a <%= @adj.sample %>
            <strong><%= link_to "#{book_duet.musician} & #{book_duet.author}", book_duet_path(book_duet) %></strong> Book Duet
            <%= render partial: "explicit_label", locals: {filter_level: book_duet.filter_level} %></h4></li>
        <% end %>
      </ul>
    <% end %>
  </div>
</div>
